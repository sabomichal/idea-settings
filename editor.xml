<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Source Code Pro" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern pattern="\b(todo|fixme) sabo\b.*" />
    <filter name="todo sabo">
      <pattern index="1" />
    </filter>
  </component>
</application>